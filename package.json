{
  "name": "my-app",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "yarn build:tsc && yarn build:generator && yarn start:server",
    "start:server": "node ./randoms/server.js",
    "build:tsc": "tsc --rootDir ./src --outDir randoms --diagnostics && babel randoms --out-dir randoms",
    "build:generator": "randoms-generator"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@randoms-pkg/code-generator": "^2.2.2",
    "@types/express": "^4.17.15",
    "babel-plugin-module-resolver": "^4.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@randoms/core": "https://github.com/RandomsDev/randoms-core.git"
  }
}
